{
    "compilerOptions": {
        "declaration": true,
        "module": "commonjs",
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "removeComments": true,
        "sourceMap": true,
        "target": "es2015",
        "strictNullChecks": true,
        "noImplicitAny": true
    },
    "exclude": [
        "node_modules",
        "**/*.spec.ts"
    ]
}
