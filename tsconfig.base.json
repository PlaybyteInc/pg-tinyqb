{
  "compilerOptions": {
        "declaration": true,
        "module": "commonjs",
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "removeComments": true,
        "sourceMap": true,
        "target": "es2015"
    },
    "exclude": [
        "node_modules",
        "**/*.spec.ts"
    ]
}
